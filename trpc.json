{
    "openapi": "3.0.0",
    "info": {
        "version": "1",
        "title": "TransmissionRPC",
        "license": {
            "name": "MIT"
        }
    },
    "servers": [
        {
            "url": "192.168.130.3:9091"
        }
    ],
    "paths": {
        "/transmission/rpc": {
            "post": {
                "summary": "The one and only path",
                "operationId": "torrent-get",
                "tags": [
                    "user"
                ],
                "parameters": [
                    {
                        "name": "method",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "arguments",
                        "in": "header",
                        "required": false,
                        "schema": {
                            "type": "object"
                        }
                    },
                    {
                        "name": "tag",
                        "in": "header",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Details about a torrent",
                        "headers": {
                            "x-next": {
                                "description": "A link to the next page of responses",
                                "schema": {
                                    "type": "string"
                                }
                            }
                        },
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "unexpected error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
